{
  "name": "minesweeper-app",
  "version": "1.0.0",
  "description": "Minesweeper React component for Jekyll integration",
  "main": "dist/minesweeper.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "build:jekyll": "webpack --mode production --env target=jekyll",
    "dev:jekyll": "webpack --mode development --watch --env target=jekyll",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.0",
    "mini-css-extract-plugin": "^2.7.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
